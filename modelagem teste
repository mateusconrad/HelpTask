



chamado{
	id_chamado:		(o número do chamado)
	status:			(1,2,3 ou 4 )
	titulochamado:	(title)
	descricao:		(descrição do chamado)
	usuario:		(id do usuario que fez)
}

usuario{
	id:				(id do user)
	login:			(login do usuario)
	nome: 			(nome do user)
	senha:			(senha )
	avatar:			(foto de pergil do usuario)
}


